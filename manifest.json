{
  "manifest_version": 2,
  "name": "ReadTime",
  "version": "0.0.1",
  "description": "A chrome extension to estimate the time you need to read this article.",

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
      "default_title": "ReadTime",
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  }
}
